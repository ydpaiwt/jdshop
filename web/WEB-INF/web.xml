    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>com.cn.sxzx.jdteam.login.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
